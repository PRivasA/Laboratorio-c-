using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System. Threading.Tasks;
namespace Condicionescompuestas7
{
    class program
    {
        static void Main(string [] args)
        {
            int x, y;
            string linea;
            Console.Write("ingrese coordenada x:");
            linea= Console.ReadLine();
            x = int.Parse(linea);
            Console.Write("ingrese coordenada y:);
            linea = Console.ReadLine();
            y = int.Parse(linea);
            if (x > 0 && y > 0)
            {
                Console.Write("se encuentra el primer cuadrante");
            }
            else
            {
                if (x > 0 && y > 0)
                {
                    Console.Write("se encuetra el segundo cuadrante");

                }
                else
                {
                    if (x < 0 && y < 0)
                    {
                        Console.Write("se encuentra el tercer cuadrante");
                    }
                    else
                    {
                        Console.Write("se encuentra el cuarto cuadrante");
                    }
                }
            }
            Console.ReadKey();
        }
    }
}